class Solution {
    public int removeDuplicates(int[] arr) {
        
        int count = 0;
        for(int i=1;i<arr.length;i++){
            if(arr[i] > arr[i-1]){
                count++;
                arr[count] = arr[i];
            }
        }
        
        return count+1;
        
    }
}
